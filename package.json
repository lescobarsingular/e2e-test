{
  "name": "singular-e2e",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:headed:one": "playwright test --headed --project=chromium",
    "test:ui": "playwright test --ui",
    "report": "playwright show-report",
    "auth:setup": "playwright test tests/auth.setup.ts --headed --project=auth-setup",
    "test:allure": "playwright test --project=chromium --project=firefox --project=webkit && npm run allure:generate && npm run allure:open",
    "test:allure:headed": "playwright test --headed --project=chromium --project=firefox --project=webkit && npm run allure:generate && npm run allure:open",
    "allure:generate": "./scripts/allure-generate-with-history.sh",
    "allure:generate:fresh": "allure generate allure-results -o allure-report --clean",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.0.0",
    "allure-commandline": "^2.33.0",
    "allure-playwright": "^3.0.0",
    "typescript": "^5.6.0"
  }
}
